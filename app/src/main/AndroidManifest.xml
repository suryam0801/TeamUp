<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ControlPanel.ChatRoom" />
        <activity android:name=".ControlPanel.TaskList" />
        <activity android:name=".ControlPanel.ProjectWall" />
        <activity android:name=".ControlPanel.ControlPanel" />
        <activity android:name=".login.SignUpActivity" />
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".ControlPanel.DisplayApplicants.ApplicantDisplay" />
        <activity android:name=".CreateProject" />
        <activity android:name=".Explore.ExploreActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>